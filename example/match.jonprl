Theorem match_example : [{A : U{i}} {B : U{i}} A -> B -> A] {
  intro; [intro, auto]; [intro, auto]; [intro, auto]; [id, auto];
  @{[A' |- A'] => assumption}
}.

Theorem match_example2 : [(A : U{i})(x : A)  A] {
  intro; [intro, auto]; [assert [member(x; A)], auto]; [auto, id];
  @{[member(X; A') |- A'] => witness [X]; assumption}
}.
